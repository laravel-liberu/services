    "keywords": [
        "laravel-liberu",
        "laravel",
        "services",
        "service-management"
    ],
    "authors": [
        {
            "name": "Adrian Ocneanu",
            "email": "aocneanu@gmail.com",
            "homepage": "https://laravel-liberu.com",
            "role": "Developer"
        }
    ],
    "homepage": "https://github.com/laravel-liberu/services",
    "type": "library",
    "license": "MIT",
    "require": {
        "laravel-liberu/core": "^8.0",
        "laravel-liberu/dynamic-methods": "^2.0",
        "laravel-liberu/forms": "^4.0",
        "laravel-liberu/helpers": "^2.0",
        "laravel-liberu/measurement-units": "^3.0",
        "laravel-liberu/tables": "^4.0"
